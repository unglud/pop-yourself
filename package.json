  "author": "Aleksandr \"unglued\" Matrosov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unglud/pop-yourself/issues"
  },
  "homepage": "https://github.com/unglud/pop-yourself#readme",
  "dependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/chrome": "0.0.154",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "depcheck": "^1.4.2",
    "react": "17.0.2",
    "react-cookie-consent": "^6.3.0",
    "react-dom": "17.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sass": "^1.2.8",
    "standard-version": "^9.3.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
